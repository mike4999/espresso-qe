$ECHO "  running the calculation with 32 H2O molecules...\c"
#$FPMD_COMMAND < h2o-32.in > h2o-32.out
#$ECHO " done"
$ECHO " skipped" # too big, don't run

# molecular dynamics calculation
cat > h2o-256.in << EOF
 &control
